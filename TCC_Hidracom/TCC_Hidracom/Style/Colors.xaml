<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TCC_Hidracom">

    <Color x:Key="BackgroundLightGray">#D9DFE5</Color>
    <SolidColorBrush x:Key="BackgroundLightGrayBrush" Color="{StaticResource BackgroundLightGray}" />

    <Color x:Key="BackgroundLightBlue">#6B87C8</Color>
    <SolidColorBrush x:Key="BackgroundLightBlueBrush" Color="{StaticResource BackgroundLightBlue}" />

    <Color x:Key="BackgroundGold">#e5c100</Color>
    <SolidColorBrush x:Key="BackgroundGoldBrush" Color="{StaticResource v}" />

</ResourceDictionary>